<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/edittext_shape"
    tools:context=".Profile_Change_Activity"
    tools:openDrawer="start"
    android:layout_margin="20dp"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="15dp"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dp"
            android:elevation="5dp"
            android:layout_gravity="top|right"
            android:layout_marginBottom="10dp"
            android:src="@drawable/close"
            android:id="@+id/close_buttton_id"
            />

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/user_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="নাম"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textPhonetic"
                    >

                </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/phone_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="ফোন নাম্বার"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textPhonetic"
                    >

                </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/email_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="ইমেইল"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textEmailAddress"
                    >

                </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                app:passwordToggleEnabled="true"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
            <EditText
                android:id="@+id/password"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="পাসওয়ার্ড দিন"
                android:padding="15dp"
                android:textStyle="bold"
                android:textSize="25sp"
                android:inputType="textPassword"
                >

            </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/voter_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="জাতীয় পরিচয় নাম্বার"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textPhonetic"
                    >

                </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/division_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="বিভাগ"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textPhonetic"
                    >
                </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/district_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="জেলা"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textPhonetic"
                    >
                </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/up_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="উপজেলা"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textPhonetic"
                    >
                </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/un_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="ইউনিয়ন"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textPhonetic"
                    >
                </EditText>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/white_border"
                android:outlineAmbientShadowColor="@android:color/darker_gray"
                android:padding="10dp"
                android:layout_marginBottom="10dp"
                >
                <EditText
                    android:id="@+id/village_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="গ্রাম"
                    android:padding="15dp"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:inputType="textPhonetic"
                    >
                </EditText>
            </com.google.android.material.textfield.TextInputLayout>

        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:background="@drawable/green_light_border"
            android:id="@+id/radio_button">
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="পুরুষ"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="@color/colorPrimary"
                android:buttonTint="@color/colorPrimary"
                tools:targetApi="lollipop"
                android:id="@+id/male"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="2dp"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="মহিলা "
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="@color/colorPrimary"
                android:buttonTint="@color/colorPrimary"
                tools:targetApi="lollipop"
                android:id="@+id/female"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="2dp"
                />
        </RadioGroup>
        <ProgressBar
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/progressbar"
            android:layout_gravity="center"
            android:indeterminateTint="@color/colorPrimary"
            tools:targetApi="lollipop"
            android:visibility="gone"
            />
        <FrameLayout
            android:layout_width="200dp"
            android:layout_height="80dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="80dp"
            android:layout_marginRight="80dp"
            android:elevation="10dp"
            android:outlineAmbientShadowColor="@android:color/darker_gray"
            android:background="@drawable/green_light_border"
            tools:targetApi="p">
            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="নিশ্চিত করুন"
                android:textColor="#000000"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textAllCaps="false"
                android:background="?attr/selectableItemBackground"
                android:id="@+id/profile_change_btn"
                />
        </FrameLayout>

    </LinearLayout>


</ScrollView>

